    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "keypair": "^1.0.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mongoose": "^5.13.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-icons": "^4.2.0",
    "validator": "^13.6.0"
  }
